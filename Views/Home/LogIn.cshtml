@model MMIv3.Models.User
<head>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0" />
</head>
<body>
    <div class="profile_page_mycontainer">
        <div class="profile_page_leftmenu">
            <div class="elipsa" style="background-image:src()"></div>
        </div>
        <div class="profile_page_middlemenu">
            <img src="~/images/koya.svg" style="float: left;" />
            <input type="text" placeholder="Search tags in profile" style="float: left;" />
            <div class="profile_break" style="width: 100%; height: 40px; background-color: #4B4E6D; clear: both;"></div>
            <div class="profile_info">
                <div class="profileicon_elipsa">
                    <img src="~/images/channels4_profile.jpg" style="width: 100%; height: 100%; border-radius: 50%;" /><br />
                    <h1 class="profile_name" style="text-align: center;">Beluga</h1>
                </div>
            </div>
            <div class="profile_aboutme">
                <h1>About me...</h1>
            </div>
            <div class="profile_posts">
                <h1>Posts...</h1>
            </div>
        </div>
        <div class="profile_page_rightmenu" style="padding: 20px 10px;">
            <div class="friends_list">
                <p style="float: left;">Friends list:</p>
                <a class="page_link" style="float: right; color: #90EE90; text-decoration: none;"><div class="div_button"><span class="material-symbols-outlined">add</span><p>Add new</p></div></a>
                <br style="clear: both;" />
            </div>
            <div class="break_panel"></div>
            <div class="friends_requests" style="height: 50%">
                <p>Requests:</p>
            </div>
            <div class="break_panel"></div>
            <div class="user_profile">
                <a class="page_link"><div class="div_button"><span class="material-symbols-outlined">manage_accounts</span><p>Edit profile</p></div></a><br />
                <a class="page_link"><div class="div_button"><span class="material-symbols-outlined">settings</span><p>Settings</p></div></a><br />
                <a class="page_link" style="color: #E99595; text-decoration: none;"><div class="div_button"><span class="material-symbols-outlined">logout</span><p>Log out</p></div></a>
            </div>
        </div>
    </div>
</body>
